    "compilerOptions": {
      "baseUrl": "./src",
      "paths": {
        "assets/*": ["./assets/*"],
        "components/*": ["./components/*"],
        "configs/*": ["./configs/*"],
        "data/*": ["./data/*"],
        "hooks/*": ["./hooks/*"],
        "providers/*": ["./providers/*"],
        "services/*": ["./services/*"],
        "shared/*": ["./shared/*"],
        "store/*": ["./store/*"],
        "utils/*": ["./utils/*"]
      },
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": false,  // TODO: true
    "noImplicitAny": false,  // TODO: true
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": false,
    "noEmit": false,
    "jsx": "react-jsx"
  },
  "include": [
    "src"
  ]
}
